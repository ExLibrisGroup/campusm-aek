element = 'reset'
/*******************************
             Reset
*******************************/
/* Border-Box */
*, *:before, *:after
  box-sizing: inherit

html
  box-sizing: border-box

/* iPad Input Shadows */
input[type="text"], input[type="email"], input[type="search"], input[type="password"]
  -webkit-appearance: none
  -moz-appearance: none
  /* mobile firefox too! */

